<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="department" class="com.webonise.Department">
       <property name="dept_name" value="Development"></property>
   </bean>
   
   <bean id="student" class="com.webonise.Student">
       <property name="name" value="Tanvi.."></property>
       <property name="sno" value="225"></property>
       <property name="dept">
       <ref bean="department"/>
       </property>
   </bean>

</beans>